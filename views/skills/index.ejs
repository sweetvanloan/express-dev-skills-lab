<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My Dev Skills</title>
</head>

<body>
    <h1>My Dev Skills</h1>
    <ul>
        <% skills.forEach(function(skill, idx) { %>
            <li>
                <a href="/skills/<%=idx %>">
                    <%= skill.text %>

                </a>
                -
                <%= skill.done ? "Learned" : "Not Learned" %>
            </li>
            <% }) %>
    </ul>
</body>

</html>